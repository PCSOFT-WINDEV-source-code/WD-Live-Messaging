   -
     internal_properties : 
     code : |1+
      GLOBAL	
      	CONSTANT
      		// Name of the sockets used
      		csPresenceSocketName = "PresenceSocket"			// Socket used to detect the presence of a user
      		csRequestedSocketName = "RequestSocket"			// Socket used to perform the requests of presence
      		
      		// Numbers of the ports associated with the sockets
      		cnPresenceSocketPort = 5010					// Port associated with csNamePresenceSocket
      		cnRequestSocketPort = 5011					// Port associated with csNameRequestSocket
      		
      		// Protocol message
      		csACKOK = "OK"								// Confirmation message
      		csNOTAVAIL = "NOT_AVAIL"						// User not available
      		csENDCOM = "REQUEST_STOP"					// Ask for the end of communication
      		csREQ = "QRY"								// Connection request
      	END
      
      HSetREP(False)
      
      //
      // Transfer files in local if a network disk is currently used
      //
      IF fDriveInfo(fExtractPath(fExeDir(),fDrive),fdDriveType) = fdRemote OR NetworkDirName(fExtractPath(fExeDir(),fDrive)) <> "" THEN
      		
      	// generates a local directory
      	gsFilesDir is string = CompleteDir(SysDir(srLocalAppData)) + ProjectInfo(piConfiguration) + "\"
      	
      	// creates the directory
      	fMakeDir(gsFilesDir)
      	
      	// Modifies the path of the data files
      	HSubstDir("*", gsFilesDir)
      END
      
      // Create the Contact file if necessary
      HCreationIfNotFound(CONTACT)						
      
      // Open the presence socket
      OpenPresenceSocket()
      
      // Manage the threads manually (each procedure is not a critical section)
      ThreadMode(threadCriticalSection)
