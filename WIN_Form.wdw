   -
     code : |1-
      PROCEDURE WIN_Form(gnContactID=0)
      
      // Creation or modification
      IF gnContactID<>0 THEN
      	// Modify a contact
      	HReadSeekFirst(CONTACT,CONTACTID,gnContactID)
      	IF HFound(CONTACT) THEN
      		// Implement the record
      		FileToScreen(WIN_Form,CONTACT)
      	END
      ELSE
      	// Create a new contact
      	HReset()				// Clear the variables corresponding to the record	
      	Reset()				// Clear the controls
      END
      
      MyWindow..ReturnedValue = False
