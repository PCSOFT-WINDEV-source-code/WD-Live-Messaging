     p_codes :
      -
        code : |1-
         // Retrieve the content of the file
         ScreenToFile(WIN_Form,CONTACT)
         
         // It is either an IP address or a DNS name
         IF Edit_IP_Address~="" AND Edit_DNS_Address~="" THEN
         	Info(<§$0000§>)
         	ReturnToCapture(Edit_IP_Address)
         END
         
         // Check whether an IP address is specified
         IF Edit_DNS_Address~="" AND (Edit_IP_Address~="" OR Position(Edit_IP_Address,".")<1) THEN
         	Info(<§$0002§>)
         	ReturnToCapture(Edit_IP_Address)
         END
         
         // Check whether a contact name is specified
         IF Edit_Name_Contact~="" THEN
         	Info(<§$0003§>)
         	ReturnToCapture(Edit_Name_Contact)
         END
         
         // Modify or create a contact?
         IF gnContactID = 0 THEN
         	// Mode for creating a contact
         	HAdd(CONTACT)
         	IF ErrorOccurred THEN
         		// Error while adding the contact
         		Error(<§$0004§>,HErrorInfo())
         		// Return to the form
         		RETURN
         	END
         ELSE
         	// Mode for modifying a contact
         	HModify(CONTACT)
         	IF ErrorOccurred THEN		
         		// Error while adding the contact
         		Error(<§$0005§>,HErrorInfo())		
         		// Return to the form
         		RETURN
         	END
         END
         
         // Confirmation message
         Info(<§$0006§>)
         
         // Close the window
         Close("", True)
