     code : |1+
      PROCEDURE WIN_Message(gnContactID,gsSocketName)
      
      gnEventID is int
      gsThreadName is string = gsSocketName
      
      // Search for the contact form
      HReadSeekFirst(CONTACT,CONTACTID,gnContactID)
      IF HFound(CONTACT) THEN
      	// The contact was found, indicate his name
      	// Note: the name is indicated from the list of contacts
      	Static_Connection = Replace(Static_Connection,"XX",CONTACT.Name)
      ELSE
      	// The contact is unknown	
      	Static_Connection = Replace(Static_Connection,"XX",<§$0003§>)
      END
