   -
     name : ReadMessage
     internal_properties : CAAAAAgAAADtr/eeEfmLWlHFe2k5RIkXE+H+mEryrGU6CBK/uKmmninibeA/V+qLnN5cY53Y0v65RhFPF92lLzjcSDhpIOVbtV/GB26JrNhTzLI2bJR/VPRiXG11TT5A2OQme3mjoky3uOSXKUXqliS+4bBSngbIvNhKk6uDEplw3O7jrE5u0I5Q1K0w3XRh5hSRjwmMGbMonMtrcHC8TMJwDfImaZmSTwBOTHMbe6zsC1mw5DbuT52yXg==
     procedure_id : 1760488368587379357
     type_code : 14
     code : |1+
      PROCEDURE ReadMessage(sReadSocketName)
      
      sMessage is string			// Message received
      
      // Read as long as the thread is active
      // Note: A locking function is used during an undefined duration in the loop: no need to monopolize the processor
      LOOP	
      	// Read a message (locking function during an undefined duration)	
      	sMessage = SocketRead(sReadSocketName,True)
      	
      	// Check the message	
      	IF NoSpace(sMessage)<>"" THEN	
      		// There is a message asking for the communication to stop
      		IF sMessage~=csENDCOM THEN
      								
      			// Close the listening socket	
      			SocketClose(gsSocketName)
      
      			// Ask to close the window	
      			ExecuteMainThread(WIN_Message.EndCommunication, True)
      			BREAK			
      		ELSE				
      			
      			EDT_RTF = ""
      			RTFAdd(EDT_RTF, "#" + CONTACT.Name + "#: ")
      			RTFAdd(EDT_RTF, sMessage)
      			EDT_RTF = Replace(EDT_RTF, "#" + CONTACT.Name + "#: ", "\b1 " + CONTACT.Name + "\b0: ")
      			
      			// Displaying the message			
      			TableAddLine(Message_Table, EDT_RTF)
      			// Position on the last row of the table (corresponding to the last message)
      			TableSelectPlus(Message_Table,TableCount(Message_Table))
      		END
      	END
      	
      	Multitask(-2)
      END
      
      // Stop the listening thread
      ThreadStop(gsThreadName,500)
      Multitask(-30)
     type : 458752
   -
     name : WriteMessage
     internal_properties : CAAAAAgAAADtg4MFrEil8FPR5obU+N1dtKtviud2O4wR+zeZEA58TNS77bfyw0wHK1DtUrjUEME42ljRbSK28T6w5ECdY8TnctcFiD+pkWE9wwSnW3EI+2LU510elHCbxBrSSOmy4B0wjjiqywDDY2CeGYNAXfKjs9YCYJ94gbBuHqxk/G9ZFs0VLWNxfcB3gUd782LmhJh4/A==
     procedure_id : 1760488368587444893
     type_code : 14
     code : |1+
      PROCEDURE WriteMessage(sMessage="test")
      
      // Procedure used to send messages via socket
      
      // Send the message
      // Note: this function is not a locking function
      SocketWrite(gsSocketName,sMessage)
      IF ErrorOccurred THEN
      	// Error while sending the message
      	Error(<§$0007§>,ErrorInfo())
      ELSE
      	// If this is not a message asking for the end of connection
      	IF NOT sMessage~=csENDCOM THEN
      		// Also send the message locally 			
      		
      		EDT_RTF = ""
      		RTFAdd(EDT_RTF, "#" + NetworkUser() + "#: ")
      		RTFAdd(EDT_RTF, sMessage)
      		EDT_RTF = Replace(EDT_RTF, "#" + NetworkUser() + "#: ", "\b1 " + NetworkUser() + "\b0: ")
      		
      		// Displaying the message			
      		TableAddLine(Message_Table, EDT_RTF)
      	END
      END
      
     type : 458752
   -
     name : Closing
     internal_properties : CAAAAAgAAADmhxqgBCBBXNFDfyp45J+UH13we/8YTr3JGMXrg63OTKZRwA6VrZhUvn/OblmpCfShQcnjSSy2Vq/4FLaYXlV73zxT5PbratdXpa0T1Hecjj4/SHnoNh9yQomZ0MfA7EU39l0GdUKEDDzdRi+uo1TlGtxX/NecnunqZQW0DGNKnEWFqrNrVYsSAWalR3woSGczr3PHsfCbyQliI8A/Pym1R4JrHA==
     procedure_id : 1760488368587510429
     type_code : 14
     code : |1+
      PROCEDURE Closing(bRequestStop=False)
      
      // Send a message to the other user indicating the end of communication
      IF bRequestStop THEN 
      	WriteMessage(csENDCOM)
      END
      
      // Stop the listening thread
      ThreadStop(gsThreadName,500)
      
      // Close the listening socket	
      SocketClose(gsSocketName)
     type : 458752
   -
     name : EndCommunication
     procedure_id : 1760488368587575965
     type_code : 14
     code : |1+
      PROCEDURE EndCommunication(bRemote is boolean = False)
      
      	IF bRemote THEN
      		// The communication was stopped by the correspondent			
      		Info(<§$0005§>)
      	END
      	
      	// Close the window
      	Close
      
      
     type : 458752
